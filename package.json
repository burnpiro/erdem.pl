    "develop": "npm run clean && gatsby develop",
    "serve": "gatsby serve",
    "custom": "gatsby info --clipboard",
    "build": "npm run clean && gatsby build",
    "deploy": "npm run clean && gatsby build --prefix-paths && cp -R .circleci public && gh-pages -d public",
